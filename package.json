{
  "name": "ac-config-vue",
  "description": "Auth Connect Configuration Sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "~4.8.0",
    "@capacitor/angular": "~2.0.3",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "~4.8.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "~4.8.0",
    "@capacitor/keyboard": "~4.1.1",
    "@capacitor/preferences": "~4.0.2",
    "@capacitor/splash-screen": "~4.2.0",
    "@capacitor/status-bar": "4.1.1",
    "@ionic-enterprise/auth": "~4.0.0",
    "@ionic/vue": "~7.0.0",
    "@ionic/vue-router": "~7.0.0",
    "core-js": "^3.6.5",
    "ionicons": "~7.1.0",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@capacitor/assets": "~2.0.4",
    "@capacitor/cli": "~4.8.0",
    "@ionic/prettier-config": "~2.1.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "~0.30.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "~8.0.3",
    "jsdom": "~22.0.0",
    "prettier": "~2.8.7",
    "pretty-quick": "~3.1.3",
    "typescript": "~5.0.3",
    "vite": "^4.1.0",
    "vitest": "~0.30.1",
    "vue-tsc": "^1.0.24"
  },
  "prettier": "@ionic/prettier-config"
}
