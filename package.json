{
  "name": "ac-config-vue",
  "description": "Auth Connect Configuration Sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run clean && npm run trapeze && npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "clean": "git checkout android/app/build.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run",
    "trapeze": "(export $(cat .env | xargs) && trapeze run -y config.yaml)"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/angular": "~2.0.3",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/preferences": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic-enterprise/auth": "~5.0.1",
    "@ionic/vue": "~7.2.0",
    "@ionic/vue-router": "~7.2.0",
    "core-js": "^3.6.5",
    "ionicons": "~7.1.0",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@capacitor/assets": "~2.0.4",
    "@capacitor/cli": "^5.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@trapezedev/configure": "~7.0.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-v8": "~0.33.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "~8.0.3",
    "jsdom": "~22.1.0",
    "lint-staged": "~13.2.3",
    "prettier": "~3.0.0",
    "typescript": "~5.1.3",
    "vite": "^4.1.0",
    "vitest": "~0.33.0",
    "vue-tsc": "^1.0.24"
  },
  "prettier": "@ionic/prettier-config"
}
