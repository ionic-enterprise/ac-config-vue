{
  "name": "ac-config-vue",
  "description": "Auth Connect Configuration Sample",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run trapeze && npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "clean": "git checkout android/variables.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "config": "(export $(cat .env | xargs); npm run clean; node scripts/buildConfig.js ; npm run trapeze)",
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "trapeze": "(export $(cat .env | xargs) && trapeze run -y config.yaml)"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/angular": "~2.0.3",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-enterprise/auth": "^6.0.4",
    "@ionic/vue": "^8.4.1",
    "@ionic/vue-router": "^8.4.1",
    "core-js": "^3.39.0",
    "ionicons": "~7.4.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.1",
    "@capacitor/cli": "^6.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@trapezedev/configure": "^7.1.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "~2.4.5",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "~15.2.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "prettier": "@ionic/prettier-config"
}
